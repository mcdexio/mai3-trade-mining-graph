{
    "name": "mai3-trade-mining-graph",
    "version": "0.1.0",
    "scripts": {
      "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
      "prepare:kovan": "mustache config/kovan.json subgraph.template.yaml > subgraph.yaml",
      "create": "graph create mcarloai/mai3-trade-mining-graph --node https://api.thegraph.com/deploy/",
      "create-local": "graph create mcarloai/mai3-trade-mining-graph --node http://10.30.206.10:8020",
      "codegen": "graph codegen",
      "build": "graph build",
      "deploy": "graph deploy mcarloai/mai3-trade-mining-graph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
      "deploy-local": "graph deploy mcarloai/mai3-trade-mining-graph --ipfs http://10.30.206.10:5001 --node http://10.30.206.10:8020"
    },
    "devDependencies": {
      "@graphprotocol/graph-cli": "^0.18.0",
      "@graphprotocol/graph-ts": "^0.18.0",
      "mustache": "^3.1.0"
    }
  }